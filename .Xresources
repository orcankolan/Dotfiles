! special
*foreground:   #e6e1dc
*background:   #2b2b2b
*cursorColor:  #e6e1dc

! black
*color0:       #2b2b2b
*color8:       #5a647e

! red
*color1:       #da4939
*color9:       #da4939

! green
*color2:       #a5c261
*color10:      #a5c261

! yellow
*color3:       #ffc66d
*color11:      #ffc66d

! blue
*color4:       #6d9cbe
*color12:      #6d9cbe

! magenta
*color5:       #b6b3eb
*color13:      #b6b3eb

! cyan
*color6:       #519f50
*color14:      #519f50

! white
*color7:       #e6e1dc
*color15:      #f9f7f3

! === Emacs ===
Emacs.font: Hack-10

! === URxvt ===
URxvt.intensityStyles:	false
URxvt.background:       [70]#000000
URxvt.depth:		32
URxvt.font: xft:Hack:size=10
URxvt.letterSpace:-1
URxvt.iso14755: false
URxvt.iso14755_52: false
!URxvt.font: xft:monaco:size=10
URxvt.scrollBar:	false
URxvt.cursorColor: magenta

!Extentions
URxvt.perl-ext-common: default,selection-to-clipboard,font-size,keyboard-select

! === dmenu ===
!-nb '#151515' -nf '#999999' -sb '#f00060' -sf '#000000' -fn '-*-*-medium-r-normal-*-*-*-*-*-*-100-*-*'
!-nb '#2f343f' -nf '#f3f4f5' -sb '#9575cd' -sf '#f3f4f5' -fn '-*-*-medium-r-normal-*-*-*-*-*-*-100-*-*'


! === rofi ===

rofi.modi:                           ssh,window,drun,run
rofi.sidebar-mode:                   true
rofi.terminal:                       termite
rofi.ssh-client:                     ssh
rofi.ssh-command:                    {terminal} -e "{ssh-client} {host}"
rofi.opacity:                        70
rofi.width:                          50
rofi.lines:                          8
rofi.columns:                        1
rofi.font:                           Inconsolata 20
rofi.fg:                             #5294E2
rofi.bg:                             #5d729f70
rofi.fg-active:                      #34405a
rofi.fg-urgent:                      #34405a
rofi.hlfg-active:                    #5e73a0
rofi.hlfg-urgent:                    #5e73a0
rofi.bg-active:                      #34405a
rofi.bg-urgent:                      #34405a
rofi.hlbg-active:                    #5d729f
rofi.hlbg-urgent:                    #5d729f
rofi.bgalt:                          #5671a0
rofi.hlfg:                           #34405a
rofi.hlbg:                           #34405a
rofi.bc:                             #789ee1
! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: #35394433,  #FFFFFF,  #353944,  #5294E2,  #FFFFFF

!                  'background', 'border'
rofi.color-window: #35394480,      #404552
rofi.bw:                             10
rofi.location:                       0
rofi.padding:                        20
rofi.levenshtein-sort:               true
rofi.case-sensitive:                 false
rofi.fuzzy:                          false
rofi.line-margin:                    2
rofi.separator-style:                solid
rofi.hide-scrollbar:                 true
rofi.markup-rows:                    false
rofi.scrollbar-width: 8

!keyboard-select
URxvt.keysym.M-z: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keyboard-select.clipboard:true
