alias odtu-tm="cd /var/www/odtu-tm-org"
alias odtu-psm="cd /var/www/odtu-psm-org"
alias odtu-com="cd /var/www/odtu-tm-com"
alias www="cd /var/www"
alias psm="cd /var/www/psm-portal"
alias ndt="cd /var/www/ndt-portal"
alias rc-backup="rclone -q sync gdrive:laravel-backups ~/laravel-backups"
alias uz-latest="find ~/laravel-backups -type f | sort -nr | cut -d: -f2- | head -1 | xargs unzip -d ~/laravel-backups -q"
alias sync-storage-psm="rsync --delete --recursive  ~/laravel-backups/opt/homebrew/var/www/psm-portal/storage/app/ /var/www/psm-portal/storage/app"
alias cp-storage-psm="cp -rf ~/laravel-backups/opt/homebrew/var/www/psm-portal/storage/app/ /var/www/psm-portal/storage"
alias upto-db="sudo mysql psm_portal < ~/laravel-backups/db-dumps/mysql-psm_portal.sql"
