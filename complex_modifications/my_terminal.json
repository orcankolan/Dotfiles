{
  "title": "Double tap left_command to open iterm",
  "rules": [
    {
      "description": "Double tap control button to open iterm",
      "manipulators": [
        {
          "conditions": [
            {
              "name": "right_command pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open -a iTerm.app"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "right_command pressed",
                "value": 1
              }
            }
          ],
          "to_delayed_action": {
            "to_if_canceled": [
              {
                "set_variable": {
                  "name": "right_command pressed",
                  "value": 0
                }
              }
            ],
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "right_command pressed",
                  "value": 0
                }
              }
            ]
          },
          "type": "basic"
        }
      ]
    }
  ]
}
