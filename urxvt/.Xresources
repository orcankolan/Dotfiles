! special
*.foreground:   #e6e1dc
*.background:   #2b2b2b
*.cursorColor:  #e6e1dc

! black
*.color0:       #2b2b2b
*.color8:       #5a647e

! red
*.color1:       #da4939
*.color9:       #da4939

! green
*.color2:       #a5c261
*.color10:      #a5c261

! yellow
*.color3:       #ffc66d
*.color11:      #ffc66d

! blue
*.color4:       #6d9cbe
*.color12:      #6d9cbe

! magenta
*.color5:       #b6b3eb
*.color13:      #b6b3eb

! cyan
*.color6:       #519f50
*.color14:      #519f50

! white
*.color7:       #e6e1dc
*.color15:      #f9f7f3

URxvt.intensityStyles:	false
URxvt.background:       [70]#000000
URxvt.depth:		32
URxvt.font: xft:Inconsolata\ for\ Powerline:size=13
URxvt.scrollBar:	false
!URxvt.cursorColor: white

!Extentions
URxvt.perl-ext-common: selection-to-clipboard,resize-font

!URxvt.keysym.C-minus:     resize-font:smaller                                                      
!URxvt.keysym.C-plus:      resize-font:bigger                                                       
!URxvt.keysym.C-equal:     resize-font:reset                                                        
!URxvt.keysym.C-question:  resize-font:show

URxvt.resize-font.smaller: C-minus
URxvt.resize-font.bigger: C-plus
URxvt.resize-font.reset: C-equal
Urxvt.resize-font.show: C-question
